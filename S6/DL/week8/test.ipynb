    "@d2l.add_to_class(MLPScratch)\n",
    "def forward(self, X):\n",
    "X = X.reshape((-1, self.num_inputs))\n",
    "H = relu(torch.matmul(X, self.W1) + self.b1)\n",
    "return torch.matmul(H, self.W2) + self.b2"
